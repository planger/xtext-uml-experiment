grammar org.eclipse.papyrusrt.uml.umltext.UMLText with org.eclipse.xtext.common.Terminals

import "http://www.eclipse.org/uml2/5.0.0/UML" as uml 

// generate uMLText "http://www.eclipse.org/papyrusrt/uml/umltext/UMLText"
Model returns uml::Model :
	'model' {uml::Model} name=ID '{'
		packagedElement+=Package*
	'}'
;

Package returns uml::Package :
	'package' {uml::Package} name=ID '{'
		packagedElement+=Class*	
	'}'
;

Class returns uml::Class :
	'class' {uml::Class} name=ID ';'
;